								 <asp:RegularExpressionValidator ID="RegularExpressionValidator1" ControlToValidate="txtmobile" ValidationGroup="BuyLotChkEmail" runat="server" ErrorMessage="Invalid Phone Number" ForeColor="Red" Display="Dynamic"
